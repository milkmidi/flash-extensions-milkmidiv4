<?xml version="1.0"?>
<dialog title="Prefix Name" buttons="accept,cancel">
  <grid>
    <columns>
      <column/>
      <column/>
	  <column/>
    </columns>
    <row>
      <label value="Prefix: " />
      <textbox id="prefix" tabindex="1" value="_mc" multiline="false" maxlength="100" />
    </row>
	<row>
		<label value="index: " />
		<textbox id="index" tabindex="2" value="0" multiline="false" maxlength="100" />
	</row>
    <row>
      <label value="Layout: " />
		<menulist id="layout">
			<menuitem label="Left To Right" value="0" selected="true" />			
			<menuitem label="Top to Buttom" value="1" />			
		</menulist>
    </row>
  </grid>
  <separator/>
</dialog>
